{
  "name": "vue-height-collapsible",
  "version": "0.0.3",
  "author": "kunukn",
  "license": "MIT",
  "description": "Collapsible library based on CSS transition for Vue",
  "main": "dist/vue-height-collapsible.ssr.js",
  "browser": "dist/vue-height-collapsible.esm.js",
  "module": "dist/vue-height-collapsible.esm.js",
  "unpkg": "dist/vue-height-collapsible.min.js",
  "types": "vue-height-collapsible.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/vue-height-collapsible.git"
  },
  "files": [
    "dist/*",
    "vue-height-collapsible.d.ts",
    "src/**/*.vue"
  ],
  "keywords": [
    "vue",
    "collapsible",
    "accordion",
    "CSS transition",
    "library"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts --port 8084",
    "dev": "vue-cli-service serve dev/serve.ts  --port 8084",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^3.8.3",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
